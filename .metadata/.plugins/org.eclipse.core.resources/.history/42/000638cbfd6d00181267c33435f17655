
public class Robot {
	public static void main(String[] args){
		Person aibo = new Person();
		aibo.Robotname="アイボ";
		System.out.println(aibo.Robotname);

		Person asimo = new Person();
		asimo.Robotname ="アシモ";
		System.out.println(asimo.Robotname);

		Person pepper =new Person();
		pepper.Robotname ="ペッパー";
		System.out.println(pepper.Robotname);

		Person doracumon =new Person();
		doracumon.Robotname ="ドラクモン";
		System.out.println(doracumon.Robotname);
	}
}
